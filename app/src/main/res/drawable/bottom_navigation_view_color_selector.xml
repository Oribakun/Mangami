<?xml version="1.0" encoding="utf-8"?>

<!--
        This was made so the Items in the BottomNavigationBar change colors when selected
        or unselected!
-->


<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_checked="true" android:color="@color/colorSelected" />
    <item android:color="@color/colorUnselected"  />
</selector>